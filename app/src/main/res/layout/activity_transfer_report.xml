<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout
    android:id="@+id/coordinatorLayout"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.google.android.material.appbar.AppBarLayout
        app:elevation="0dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:paddingRight="10dp"
            app:popupTheme="@style/ThemeOverlay.AppCompat.Light"
            app:layout_scrollFlags="scroll|enterAlways"
            app:layout_collapseMode="pin">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/toolbar_title"
                    style="@style/TextAppearance.AppCompat.Widget.ActionBar.Title"
                    android:fontFamily="@font/overpass_regular"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text=""/>

            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>

    </com.google.android.material.appbar.AppBarLayout>

    <androidx.core.widget.NestedScrollView
        android:id="@+id/scroll1"
        android:background="#D7D8D8"
        android:scrollbars="vertical"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:id="@+id/linierLayout2"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:layout_margin="@dimen/activity_horizontal_margin"
                    android:background="#FFFFFF"
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">



                    <id.kopas.berkarya.said.ibanking.fun.TextViewCustom
                        app:textViewFont="overpass_bold.ttf"
                        android:textSize="18dp"
                        android:padding="0.0dip"
                        android:layout_marginTop="16dp"
                        android:layout_marginBottom="8dp"
                        android:textColor="#606060"
                        android:layout_gravity="center"
                        android:textAppearance="@style/TextAppearance.AppCompat.Subhead"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/activity_horizontal_margin"
                        android:layout_marginRight="@dimen/activity_horizontal_margin"
                        android:text="Transaksi Berhasil"/>

                    <RelativeLayout
                        android:layout_marginTop="8dp"
                        android:layout_marginBottom="4dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">


                        <id.kopas.berkarya.said.ibanking.fun.TextViewCustom
                            app:textViewFont="overpass_regular.ttf"

                            android:padding="0.0dip"
                            android:textColor="#606060"
                            android:layout_alignParentLeft="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/activity_horizontal_margin"
                            android:text="Tanggal"/>



                        <id.kopas.berkarya.said.ibanking.fun.TextViewCustom
                            android:id="@+id/report_tanggal"
                            app:textViewFont="overpass_regular.ttf"

                            android:padding="0.0dip"
                            android:textColor="#606060"
                            android:layout_alignParentRight="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/activity_horizontal_margin"
                            android:text="-"/>
                    </RelativeLayout>



                    <RelativeLayout
                        android:layout_marginTop="4dp"
                        android:layout_marginBottom="4dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">


                        <id.kopas.berkarya.said.ibanking.fun.TextViewCustom
                            app:textViewFont="overpass_regular.ttf"

                            android:padding="0.0dip"
                            android:textColor="#606060"
                            android:layout_alignParentLeft="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/activity_horizontal_margin"
                            android:text="Nomor Refrensi"/>



                        <id.kopas.berkarya.said.ibanking.fun.TextViewCustom
                            android:id="@+id/report_norefrensi"
                            app:textViewFont="overpass_regular.ttf"

                            android:padding="0.0dip"
                            android:textColor="#606060"
                            android:layout_alignParentRight="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/activity_horizontal_margin"
                            android:text="-"/>
                    </RelativeLayout>



                    <ImageView
                        android:layout_marginTop="@dimen/activity_vertical_margin"
                        android:layout_marginBottom="@dimen/activity_vertical_margin"
                        android:layout_marginLeft="@dimen/activity_horizontal_margin"
                        android:layout_marginRight="@dimen/activity_horizontal_margin"
                        android:layout_centerHorizontal="true"
                        android:layout_centerVertical="true"
                        android:layout_width="match_parent"
                        android:layout_height="5dp"
                        android:src="@drawable/dotted"
                        android:layerType="software" />




                    <RelativeLayout
                        android:layout_marginTop="4dp"
                        android:layout_marginBottom="4dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">


                        <id.kopas.berkarya.said.ibanking.fun.TextViewCustom
                            app:textViewFont="overpass_regular.ttf"

                            android:padding="0.0dip"
                            android:textColor="#606060"
                            android:layout_alignParentLeft="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/activity_horizontal_margin"
                            android:text="Sumber Dana"/>



                        <id.kopas.berkarya.said.ibanking.fun.TextViewCustom
                            android:id="@+id/report_sumberdana_nama"
                            app:textViewFont="overpass_regular.ttf"

                            android:padding="0.0dip"
                            android:textColor="#606060"
                            android:layout_alignParentRight="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/activity_horizontal_margin"
                            android:text="-"/>


                        <id.kopas.berkarya.said.ibanking.fun.TextViewCustom
                            android:id="@+id/report_sumberdana_norek"
                            app:textViewFont="overpass_regular.ttf"

                            android:padding="0.0dip"
                            android:textColor="#606060"
                            android:layout_below="@+id/report_sumberdana_nama"
                            android:layout_alignParentRight="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/activity_horizontal_margin"
                            android:text="-"/>

                    </RelativeLayout>



                    <RelativeLayout
                        android:layout_marginTop="4dp"
                        android:layout_marginBottom="4dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">


                        <id.kopas.berkarya.said.ibanking.fun.TextViewCustom
                            app:textViewFont="overpass_regular.ttf"

                            android:padding="0.0dip"
                            android:textColor="#606060"
                            android:layout_alignParentLeft="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/activity_horizontal_margin"
                            android:text="Jenis Transaksi"/>



                        <id.kopas.berkarya.said.ibanking.fun.TextViewCustom
                            android:id="@+id/report_jenistransaksi"
                            app:textViewFont="overpass_regular.ttf"

                            android:padding="0.0dip"
                            android:textColor="#606060"
                            android:layout_alignParentRight="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/activity_horizontal_margin"
                            android:text="-"/>
                    </RelativeLayout>



                    <RelativeLayout
                        android:layout_marginTop="4dp"
                        android:layout_marginBottom="4dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">


                        <id.kopas.berkarya.said.ibanking.fun.TextViewCustom
                            app:textViewFont="overpass_regular.ttf"

                            android:padding="0.0dip"
                            android:textColor="#606060"
                            android:layout_alignParentLeft="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/activity_horizontal_margin"
                            android:text="Nomor Rekening"/>



                        <id.kopas.berkarya.said.ibanking.fun.TextViewCustom
                            android:id="@+id/report_rektujuan_nama"
                            app:textViewFont="overpass_regular.ttf"

                            android:padding="0.0dip"
                            android:textColor="#606060"
                            android:layout_alignParentRight="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/activity_horizontal_margin"
                            android:text="-"/>


                        <id.kopas.berkarya.said.ibanking.fun.TextViewCustom
                            android:id="@+id/report_rektujuan_norek"
                            app:textViewFont="overpass_regular.ttf"

                            android:padding="0.0dip"
                            android:textColor="#606060"
                            android:layout_below="@+id/report_rektujuan_nama"
                            android:layout_alignParentRight="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/activity_horizontal_margin"
                            android:text="-"/>

                    </RelativeLayout>





                    <ImageView
                        android:layout_marginTop="@dimen/activity_vertical_margin"
                        android:layout_marginBottom="@dimen/activity_vertical_margin"
                        android:layout_marginLeft="@dimen/activity_horizontal_margin"
                        android:layout_marginRight="@dimen/activity_horizontal_margin"
                        android:layout_centerHorizontal="true"
                        android:layout_centerVertical="true"
                        android:layout_width="match_parent"
                        android:layout_height="5dp"
                        android:src="@drawable/dotted"
                        android:layerType="software" />



                    <RelativeLayout
                        android:layout_marginTop="8dp"
                        android:layout_marginBottom="4dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">


                        <id.kopas.berkarya.said.ibanking.fun.TextViewCustom
                            app:textViewFont="overpass_regular.ttf"

                            android:padding="0.0dip"
                            android:textColor="#606060"
                            android:layout_alignParentLeft="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/activity_horizontal_margin"
                            android:text="Nominal"/>



                        <id.kopas.berkarya.said.ibanking.fun.TextViewCustom
                            android:id="@+id/report_nominal"
                            app:textViewFont="overpass_bold.ttf"
                            android:textSize="14dp"
                            android:padding="0.0dip"
                            android:textColor="#606060"
                            android:layout_alignParentRight="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/activity_horizontal_margin"
                            android:text="-"/>
                    </RelativeLayout>



                    <RelativeLayout
                        android:layout_marginTop="4dp"
                        android:layout_marginBottom="4dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">


                        <id.kopas.berkarya.said.ibanking.fun.TextViewCustom
                            app:textViewFont="overpass_regular.ttf"

                            android:padding="0.0dip"
                            android:textColor="#606060"
                            android:layout_alignParentLeft="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/activity_horizontal_margin"
                            android:text="Biaya Admin"/>



                        <id.kopas.berkarya.said.ibanking.fun.TextViewCustom
                            android:id="@+id/report_biayaadmin"
                            app:textViewFont="overpass_bold.ttf"
                            android:textSize="14dp"
                            android:padding="0.0dip"
                            android:textColor="#606060"
                            android:layout_alignParentRight="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/activity_horizontal_margin"
                            android:text="-"/>
                    </RelativeLayout>


                    <RelativeLayout
                        android:layout_marginTop="@dimen/activity_vertical_margin"
                        android:layout_marginBottom="@dimen/activity_vertical_margin"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">


                        <View
                            android:background="@drawable/circle"
                            android:layout_marginLeft="-12dp"
                            android:layout_width="24dp"
                            android:layout_height="24dp"/>

                        <View
                            android:background="@drawable/circle"
                            android:layout_marginRight="-12dp"
                            android:layout_alignParentRight="true"
                            android:layout_width="24dp"
                            android:layout_height="24dp"/>

                        <ImageView
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            android:layout_width="match_parent"
                            android:layout_height="5dp"
                            android:src="@drawable/dotted"
                            android:layerType="software" />

                    </RelativeLayout>


                    <RelativeLayout
                        android:layout_marginTop="4dp"
                        android:layout_marginBottom="4dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">


                        <id.kopas.berkarya.said.ibanking.fun.TextViewCustom
                            app:textViewFont="overpass_regular.ttf"

                            android:padding="0.0dip"
                            android:textColor="#606060"
                            android:layout_alignParentLeft="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/activity_horizontal_margin"
                            android:text="Total"/>



                        <id.kopas.berkarya.said.ibanking.fun.TextViewCustom
                            android:id="@+id/report_total"
                            app:textViewFont="overpass_bold.ttf"
                            android:textSize="18dp"
                            android:padding="0.0dip"
                            android:textColor="#1F63CA"
                            android:layout_alignParentRight="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/activity_horizontal_margin"
                            android:text="-"/>
                    </RelativeLayout>


                    <id.kopas.berkarya.said.ibanking.fun.TextViewCustom
                        android:id="@+id/report_pesan"
                        app:textViewFont="overpass_regular.ttf"
                        android:textSize="11dp"
                        android:padding="0.0dip"
                        android:textColor="#979797"
                        android:layout_marginTop="60dp"
                        android:layout_marginBottom="20dp"
                        android:layout_gravity="center"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Struk ini merupakan bukti pembayaran yang sah"/>


                </LinearLayout>

                <com.google.android.material.button.MaterialButton
                    style="@style/Widget.MaterialComponents.Button.OutlinedButton"
                    android:backgroundTint="@color/colorDark1"
                    android:textColor="@color/colorWhite"
                    android:id="@+id/actLanjutkan"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:minHeight="60dp"
                    android:textAllCaps="true"
                    app:strokeWidth="0dp"
                    app:elevation="0dp"
                    android:elevation="0dp"
                    android:layout_margin="@dimen/activity_vertical_margin"
                    android:text="OK" />
            </LinearLayout>
        </RelativeLayout>
    </androidx.core.widget.NestedScrollView>

    <include layout="@layout/loading"/>

</androidx.coordinatorlayout.widget.CoordinatorLayout>